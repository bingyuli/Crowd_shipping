<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
    
	<managed-bean>
		<managed-bean-name>login</managed-bean-name>
		<managed-bean-class>jsf.example.Login</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>Signup</managed-bean-name>
		<managed-bean-class>jsf.example.Signup</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>profile</managed-bean-name>
		<managed-bean-class>jsf.example.Profile</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		
	</managed-bean>
	<managed-bean>
		<managed-bean-name>logout</managed-bean-name>
		<managed-bean-class>jsf.example.Logout</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>notifications</managed-bean-name>
		<managed-bean-class>jsf.example.Notifications</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>peopleNearBy</managed-bean-name>
		<managed-bean-class>jsf.example.peopleNearBy</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
		<display-name>Login</display-name>
		<from-view-id>/Login.jsp</from-view-id>
		<navigation-case>
			<from-outcome>Success</from-outcome>
			<to-view-id>/LoginSuccess.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Login</display-name>
		<from-view-id>/Login.jsp</from-view-id>
		<navigation-case>
			<from-outcome>Failed</from-outcome>
			<to-view-id>/FailedLogin.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Signup</display-name>
		<from-view-id>/Signup.jsp</from-view-id>
		<navigation-case>
			<from-outcome>Failed</from-outcome>
			<to-view-id>/FailedLogin.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Signup</display-name>
		<from-view-id>/Signup.jsp</from-view-id>
		<navigation-case>
			<from-outcome>Success</from-outcome>
			<to-view-id>/Login.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<factory>
    <render-kit-factory>org.omnifaces.renderkit.Html5RenderKitFactory</render-kit-factory>
</factory>
	
	 <!--  <lifecycle>
<phase-listener>jsf.security.AuthorizationListener</phase-listener>
</lifecycle>-->

	
</faces-config>
